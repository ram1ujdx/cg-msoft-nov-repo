package controlstructures;

public class SwitchCase {

	public static void main(String[] args) {
		
	
	int weekDay = 10;
	
	switch(weekDay) {
	case 1:
		System.out.println("Monday");
		break;
	case 2:
		System.out.println("Tuesday");
		break;
	case 3:
		System.out.println("Wednesday");
		break;
		
		
	default:
		System.out.println("Invalid Day");
		break;
		
		
	
	}
	
	}
	
}
